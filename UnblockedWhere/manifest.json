{
    "name": "UnblockedWhere",
    "description": "Gives a popup telling where a blocked video isn't blocked",
    "permissions": ["notifications", "tabs"],
    "background": {
        "page": 
            "background.html"
        
    },
    "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
    "content_security_policy": "script-src 'self' https://freegeoip.net/json/; 'https://gdata.youtube.com/feeds/api/videos/'; object-src 'self'",
    "version": "1",
    "manifest_version": 2
}
